{
  "name": "sb_reentry_app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "code-check": "yarn run --silent lint-es && yarn run --silent lint-css && yarn run --silent type-check && yarn run --silent format",
    "dev": "next dev",
    "format": "prettier * --check --ignore-unknown",
    "lint-css": "stylelint './**/*.css' --fix --cache && echo 'All stylelint checks passed!'",
    "lint-es": "eslint './**/*.{js,jsx,ts,tsx}' --fix --cache && echo 'All ESLint checks passed!'",
    "type-check": "tsc && echo 'All TypeScript compiler checks passed!'",
    "start": "next start"
  },
  "dependencies": {
    "next": "^10.0.6",
    "next-pwa": "3.1.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "twilio": "^3.55.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/node": "^14.14.22",
    "@types/qs": "6.9.4",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "stylelint": "^13.9.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "typescript": "^4.1.3",
    "webpack": "^5.19.0"
  },
  "resolutions": {
    "**/**/object-path": "^0.11.5",
    "**/**/ini": "^1.3.6",
    "**/**/source-map-resolve": "^0.6.0",
    "**/**/chokidar": "^3.0.0",
    "twilio/axios": "^0.21.1",
    "next-pwa/webpack": "^4.0.0"
  }
}
