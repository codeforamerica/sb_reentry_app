{
  "name": "sb_reentry_app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "code-check": "yarn run --silent lint-es && yarn run --silent lint-css && yarn run --silent type-check && yarn run --silent format",
    "dev": "next dev",
    "format": "prettier * --check --ignore-unknown",
    "lint-css": "stylelint './**/*.css' --fix --cache && echo 'All stylelint checks passed!'",
    "lint-es": "eslint './**/*.{js,jsx,ts,tsx}' --fix --cache && echo 'All ESLint checks passed!'",
    "type-check": "tsc && echo 'All TypeScript compiler checks passed!'",
    "start": "next start"
  },
  "dependencies": {
    "next": "^10.0.6",
    "next-pwa": "3.1.5",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "twilio": "^3.54.0"
  },
  "devDependencies": {
    "@types/node": "14.11.8",
    "@types/react": "16.9.52",
    "@typescript-eslint/eslint-plugin": "4.4.1",
    "@typescript-eslint/parser": "4.4.1",
    "eslint": "7.11.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.4",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
    "prettier": "2.1.2",
    "stylelint": "13.7.2",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "20.0.0",
    "typescript": "4.0.3",
    "webpack": "^5.19.0"
  },
  "resolutions": {
    "**/**/object-path": "^0.11.5",
    "**/**/ini": "^1.3.6",
    "axios": "^0.21.1"
  }
}
